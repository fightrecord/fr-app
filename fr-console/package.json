    "@testing-library/user-event": "^7.2.1",
    "classnames": "^2.2.6",
    "firebase": "^7.14.1",
    "luxon": "^1.23.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
